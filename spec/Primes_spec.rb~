src_path = Dir.pwd + '/../src'
$:.unshift(src_path)
require('rubygems')
require('test/unit/full')

require('Primes')

describe NaturalNumber do
  before(:all){ @nnum = NaturalNumber.new(1) }
  it "is always positive" do
    @nnum = -1
    expect{ @nnum }.to eq 1
  end
end

